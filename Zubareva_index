# PR1_Zubareva_536
<!DOCTYPE HTML>
 <html>
 <head> 
<title>Задание 536</title>
</head> 
<body> 
<script>
var n = prompt('Введите количество элементов', '');
if(n<=0){alert("Не существует!");}
var u=0;
var arr = new Array(n);
for(var i=0; i<n;i++)
{
var t= Math.floor(Math.random( ) * 30)
arr[i]= t;
document.write(arr[i] + ";");
function myFunction() 
{
 for (var i = 0; i < n - 1; i++)
           for (var j = i + 1; j < n; j++)
                {
                    if (arr[i] == arr[j])
                    {
                        u = u + 1;
                    }
                   
                }
            if (u > 0) alert("Есть");
            else alert("Нет");
}
}
  
</script>
<br></br>
<button onclick="myFunction()">Узнать,есть ли совпадающие элементы</button> 
 </body>
 </html>
